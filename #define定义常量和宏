//                                                           #define定义常量
#define Max 100
int main()
{
	int a = Max;
	return 0;
}

//                                                           #define定义宏
#include<stdio.h>
#define Max(X,Y) (X>Y?X:Y)
int main()
{
	int a = 10;
	int b = 20;
	int max = Max(a,b);
	printf("max = %d\n",max);
	return 0;
}
